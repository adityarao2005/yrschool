<AnchorPane prefHeight="500.0" prefWidth="500.0"
	style="-fx-background-color: radial-gradient(focus-angle 0.0deg, focus-distance 0.0% , center 50.0% 50.0%, radius 84.3511%, #0000005d 0.0%, #0000005d 12.7517%, #ffffff 36.0179%, #ff9f2ad1 94.7236%, #ff9f2ad1 100.0%);"
	xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="com.yrhacks.apps.yrschool.MainController">
	<children>
		<Button layoutX="140.0" layoutY="355.0" mnemonicParsing="false"
			onAction="#moveToCourses" prefHeight="100.0" prefWidth="200.0"
			text="Click To Continue">
			<font>
				<Font name="Arial" size="21.0" />
			</font>
		</Button>
		<Label layoutX="115.0" layoutY="155.0" prefHeight="75.0"
			prefWidth="250.0" text="Welcome to YRSchool" textAlignment="CENTER">
			<font>
				<Font name="System Bold Italic" size="24.0" />
			</font>
		</Label>
		<ImageView fitHeight="150.0" fitWidth="300.0"
			layoutX="165.0" layoutY="15.0" pickOnBounds="true"
			preserveRatio="true" scaleX="1.2" scaleY="1.2">
			<image>
				<Image url="@logo-removebg-preview.png" />
			</image>
		</ImageView>
		<Text layoutX="150.0" layoutY="259.0" scaleX="1.5" scaleY="1.5"
			strokeType="OUTSIDE" strokeWidth="0.0"
			text="We give resources from many different subjects such as math. We give information ranging from grade 9 curriculum to grade 12."

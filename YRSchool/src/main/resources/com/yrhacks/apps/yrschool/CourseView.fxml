<?xml version="1.0" encoding="UTF-8"?>

<?language javascript?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.shape.Line?>

<AnchorPane prefHeight="500.0" prefWidth="700.0"
	xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="com.yrhacks.apps.yrschool.CourseController">
	<children>
		<ToolBar prefHeight="40.0" prefWidth="200.0"
			AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
			AnchorPane.topAnchor="0.0">
			<items>
				<Button mnemonicParsing="false" prefHeight="30.0"
					prefWidth="30.0" style="-fx-background-radius: 100;" onAction="#moveToMain">
					<graphic>
						<Group>
							<children>
								<Line endX="10.0" strokeWidth="2.0" />
								<Line endX="5.0" endY="5.0" strokeWidth="2.0" />
								<Line endX="5.0" endY="-5.0" strokeWidth="2.0" />
							</children>
						</Group>
					</graphic>
				</Button>
			</items>
		</ToolBar>
		<TilePane hgap="20.0" prefHeight="200.0" prefWidth="200.0"
			vgap="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
			AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="40.0" fx:id="tiles">
			<padding>
				<Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
			</padding>
		</TilePane>
	</children>
</AnchorPane>

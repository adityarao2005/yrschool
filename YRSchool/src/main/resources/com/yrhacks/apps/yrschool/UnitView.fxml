<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Line?>


<AnchorPane prefHeight="500.0" prefWidth="700.0"
	xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/18"
	fx:controller="com.yrhacks.apps.yrschool.UnitController">
	<children>
		<ToolBar prefHeight="40.0" prefWidth="200.0"
			AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
			AnchorPane.topAnchor="0.0">
			<items>
				<Button mnemonicParsing="false" prefHeight="30.0"
					prefWidth="30.0" style="-fx-background-radius: 100;" onAction="#moveToCourse">
					<graphic>
						<Group>
							<children>
								<Line endX="10.0" strokeWidth="2.0" />
								<Line endX="5.0" endY="5.0" strokeWidth="2.0" />
								<Line endX="5.0" endY="-5.0" strokeWidth="2.0" />
							</children>
						</Group>
					</graphic>
				</Button>
			</items>
		</ToolBar>
		<TreeView prefWidth="200.0"
			AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
			AnchorPane.topAnchor="40.0" />
		<ScrollPane fitToWidth="true" layoutX="200.0"
			layoutY="41.0" prefHeight="200.0" prefWidth="200.0"
			AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="200.0"
			AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="40.0" />
	</children>
</AnchorPane>

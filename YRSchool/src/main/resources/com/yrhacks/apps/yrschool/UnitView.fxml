<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.web.WebView?>

<AnchorPane prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.yrhacks.apps.yrschool.UnitController">
	<children>
		<ToolBar prefHeight="40.0" prefWidth="200.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<items>
				<Button mnemonicParsing="false" onAction="#moveToCourse" prefHeight="30.0" prefWidth="30.0" style="-fx-background-radius: 100;">
					<graphic>
						<Group>
							<children>
								<Line endX="10.0" strokeWidth="2.0" />
								<Line endX="5.0" endY="5.0" strokeWidth="2.0" />
								<Line endX="5.0" endY="-5.0" strokeWidth="2.0" />
							</children>
						</Group>
					</graphic>
				</Button>
			</items>
		</ToolBar>
		<TreeView fx:id="treeView" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="40.0" />
		<WebView fx:id="webView" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="200.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="40.0" />
	</children>
</AnchorPane>
